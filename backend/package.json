{
  "name": "backend",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch index.ts",
    "syncup:db:push": "bun prisma db push --schema=src/db/prisma/syncup/schema.prisma",
    "syncup:db:generate": "bun prisma generate --schema=src/db/prisma/syncup/schema.prisma",
    "syncup:db:migrate": "bun prisma migrate dev --schema=src/db/prisma/syncup/schema.prisma",
    "soulsync:db:push": "bun prisma db push --schema=src/db/prisma/soulsync/schema.prisma",
    "soulsync:db:generate": "bun prisma generate --schema=src/db/prisma/soulsync/schema.prisma",
    "soulsync:db:migrate": "bun prisma migrate dev --schema=src/db/prisma/soulsync/schema.prisma",
    "db:push:all": "bun prisma db push --schema=src/db/prisma/syncup/schema.prisma && bun prisma db push --schema=src/db/prisma/soulsync/schema.prisma",
    "db:generate:all": "bun prisma generate --schema=src/db/prisma/syncup/schema.prisma && bun prisma generate --schema=src/db/prisma/soulsync/schema.prisma",
    "db:migrate:all": "bun prisma migrate dev --schema=src/db/prisma/syncup/schema.prisma && bun prisma migrate dev --schema=src/db/prisma/soulsync/schema.prisma"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/cron": "^1.3.0",
    "@elysiajs/swagger": "^1.3.1",
    "@prisma/client": "^6.11.1",
    "@types/nodemailer": "^6.4.17",
    "cors": "^2.8.5",
    "elysia": "latest",
    "ioredis": "^5.6.1",
    "jose": "^6.0.11",
    "nodemailer": "^7.0.5",
    "prismabox": "^1.1.24",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "prisma": "^6.11.1"
  },
  "module": "src/index.js",
  "prisma": {
    "schema": "src/db/prisma/schema.prisma"
  }
}
